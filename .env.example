# AutoPro Inspector - Environment Configuration Template
# Copy this file to .env.local and fill in your actual values

# ========================================
# Google Gemini AI Configuration
# ========================================
# Get your API key from: https://makersuite.google.com/app/apikey
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# ========================================
# Supabase Configuration
# ========================================
# Get these from your Supabase project settings
# URL format: https://your-project-id.supabase.co
VITE_SUPABASE_URL=your_supabase_project_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# ========================================
# Stripe Payment Configuration
# ========================================
# Get these from your Stripe dashboard: https://dashboard.stripe.com/
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here

# Stripe Price IDs (create these in Stripe dashboard)
VITE_STRIPE_PRO_MONTHLY_PRICE_ID=price_your_pro_monthly_id
VITE_STRIPE_PRO_YEARLY_PRICE_ID=price_your_pro_yearly_id
VITE_STRIPE_REPORT_STANDARD_PRICE_ID=price_report_standard_id
VITE_STRIPE_REPORT_EV_PRICE_ID=price_report_ev_id
VITE_STRIPE_REPORT_COMMERCIAL_PRICE_ID=price_report_commercial_id
VITE_STRIPE_REPORT_RV_PRICE_ID=price_report_rv_id
VITE_STRIPE_REPORT_CLASSIC_PRICE_ID=price_report_classic_id
VITE_STRIPE_REPORT_MOTORCYCLE_PRICE_ID=price_report_motorcycle_id

# ========================================
# Optional: Feature Flags
# ========================================
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_MOCK_PAYMENTS=true  # Set to false in production
VITE_ENABLE_ANALYTICS=false

# ========================================
# Optional: API Endpoints (if using custom backend)
# ========================================
# VITE_API_BASE_URL=https://api.yourapp.com
# VITE_BACKEND_URL=https://backend.yourapp.com
